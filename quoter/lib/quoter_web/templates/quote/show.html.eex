<h3>"<%= @quote.body %>"</h3>
<h3>- <%= @quote.author %></h3>
<div>
  <%= link "Edit", to: quote_path(@conn, :edit, @quote.id), class: "btn btn-primary" %>
  <%= link "Delete", to: quote_path(@conn, :delete, @quote), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger" %>
</div>
